<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainly"
    android:layout_width="175sp"
    android:layout_height="179sp"
    android:layout_marginEnd="10sp"
    android:background="@color/maincolour">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="217dp"
        android:layout_height="122dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.089999974"
        tools:srcCompat="@drawable/iconmusiclebar" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="46sp"
        android:layout_height="46sp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:srcCompat="@drawable/iconmusic" />

    <TextView
        android:id="@+id/titlesong"
        android:layout_width="120dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="35dp"
        android:fontFamily="@font/nsregular"
        android:text="Write song title h..."
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/artistname"
        android:layout_width="120sp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="16dp"
        android:text="Artist name"
        android:textColor="@color/textgrey"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/titlesong" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="53dp"
        android:layout_height="15dp"
        android:layout_marginStart="32dp"
        android:text="35M Plays"
        android:textColor="@color/textgrey"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/artistname"
        app:layout_constraintTop_toBottomOf="@+id/artistname"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="67dp"
        android:layout_height="15dp"
        android:text="00:50"
        android:textColor="@color/textgrey"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/artistname"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="?attr/selectableItemBackgroundBorderless"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/artistname"
        app:layout_constraintTop_toBottomOf="@+id/artistname"
        app:layout_constraintVertical_bias="0.37"
        app:srcCompat="@drawable/ic_headset" />


</androidx.constraintlayout.widget.ConstraintLayout>